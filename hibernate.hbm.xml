<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="Employ" table="HIBERDB"/>
  <id name="id">
  <column name="ID"/>
  </id>
  <property name="name">
      <column name="NAME"/>
  </property>
  <property name="age">
      <column name="AGE"/>
  </property>
  <property name="address">
      <column name="ADDRESS"/>
  </property>
</hibernate-mapping>
